# VIBES DeFi Basic DApp Environment Configuration
# Copy this file to .env and fill in your values
# ⚠️ IMPORTANT: This file contains EXAMPLE values only - replace with your actual values

# Network Configuration
# Example RPC URLs - replace with your own API keys
RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY_HERE
FALLBACK_RPC=https://api.mainnet-beta.solana.com
MAINNET_RPC=https://api.mainnet-beta.solana.com
NETWORK=mainnet-beta

# Smart Contract Program IDs (EXAMPLE - Replace with your deployed program IDs)
PRESALE_V3_PROGRAM_ID=YourPresaleProgramIdHere111111111111111111111
STAKING_PROGRAM_ID=YourStakingProgramIdHere111111111111111111111
VESTING_PROGRAM_ID=YourVestingProgramIdHere111111111111111111111

# Token Configuration (EXAMPLE - Replace with your token mints)
USDC_MINT=EPjFWdd5AufqSSqeM2qN1xzybapC8G4wEGGkZwyTDt1v
VIBES_MINT=YourVibesTokenMintHere1111111111111111111111111

# Presale Configuration
PRESALE_START_TS=0
PRESALE_END_TS=0
HARD_CAP_TOTAL=1000000000000000
FEE_RATE_BPS=50
MAX_PURCHASE_PER_WALLET=1000000000000
MIN_PURCHASE_SOL=1000000000
STAKING_APY_BPS=4000
CHARITY_RATE_BPS=300
OPTIONAL_STAKING=true

# UI Configuration
AUTO_CONNECT=false
BALANCE_REFRESH_INTERVAL=30000
PRESALE_REFRESH_INTERVAL=60000
TX_TIMEOUT=60000
